RewriteEngine On

# Force HTTPS
RewriteCond %{HTTPS} off
RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]

# Redirect non-www to www with HTTPS
RewriteCond %{HTTP_HOST} ^lesttaxi\.com [NC]
RewriteRule ^(.*)$ https://www.lesttaxi.com/$1 [L,R=301]

# Redirect any HTTP to HTTPS www
RewriteCond %{HTTPS} off
RewriteRule ^(.*)$ https://www.lesttaxi.com/$1 [L,R=301]